<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.dlwx.wisdomschool.activitys.GradeManageActivity">
    <include
        layout="@layout/toolbar"
        />
    <View
        style="@style/GaryDiveStyle"
        />
    <LinearLayout
        android:id="@+id/ll_create"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="15dp"
        android:orientation="horizontal"
        >
        <TextView
            style="@style/TextSizecontentBlack"
            android:text="+ 创建学生成绩"
            android:layout_weight="1"
            />
        <ImageView
            style="@style/Imagesmill3pic"
            android:src="@mipmap/icon_jiantourig"

            />
    </LinearLayout>
    <View
        style="@style/GaryDiveStyle"
        />
    <ExpandableListView
        android:layout_width="match_parent"
        android:id="@+id/lv_list"
        android:scrollbars="none"
        android:layout_height="wrap_content">

    </ExpandableListView>
</LinearLayout>
